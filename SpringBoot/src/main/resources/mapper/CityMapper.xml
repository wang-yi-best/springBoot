<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Configuration 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.wangyi.springBoot.modules.test.dao.CityDao">
    <select id="getCitiesByCountry" parameterType="java.lang.Integer" resultType="City">
        select * from m_city where country_id = #{countryId}
    </select>
</mapper>